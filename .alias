alias hconsole='heroku run rails console -a '
alias hpg='heroku pg:psql -a '
alias hlogs='heroku logs -t -a '
alias hrestart='heroku restart -a '
alias hmigrate='heroku run rake --trace db:migrate -a '
alias mp3='youtube-dl --extract-audio --audio-format mp3 '
