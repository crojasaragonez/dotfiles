alias tinker='rlwrap php artisan tinker'
alias old_branches='for branch in `git branch -r | grep -v HEAD`;do echo -e `git show --format="%ci %cr" $branch | head -n 1` \\t$branch; done | sort -r'
alias pdf='okular '
